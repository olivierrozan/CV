<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="epsi.cv" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CV Manager"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="CV Manager" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".vue"
            android:label="Affichage Principal" >
        </activity>
        <activity
            android:name=".AfficheCvActivity"
            android:label="Affichage de Base" >
        </activity>
        <activity
            android:name=".DetailExperienceActivity"
            android:label="title_activity_detail_experience" >
        </activity>
        <activity
            android:name=".activity_detail_competences"
            android:label="Detail des Competences" >
        </activity>
        <activity
            android:name=".activity_detail_formations"
            android:label="Detail des Formations" >
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
